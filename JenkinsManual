pipeline{
    agent any
    tools { 
        maven 'maven project'
        jdk 'jdk install path' 
    }
        stages{
            stage ('init'){
                steps{
                    echo "Init stage"
                }
            }
            stage ('build'){
                 steps{
                    echo "build stage"
                    sh label :'',script:'mvn clean package checkstyle:checkstyle'
                }

            }
            stage ('deploy'){
                 steps{
                    echo "deploy stage"
                }

            }
        }
    
}